%% Clean Up
clear; close all; clc;

%% Problem 2.3

delta = linspace(0,1000,1000) / 1000; %displacement in meters

smallFunc = 16*delta; %kN

bigFunc = 20*sqrt(delta.^2 + 8*delta + 25) - 100; %kN

%Converting to newtons

smallFunc = smallFunc * 1000;
bigFunc = bigFunc * 1000;


%Plotting
figure();
plot(delta * 1000, smallFunc, 'linewidth', 2);
hold on
plot(delta * 1000, bigFunc, 'linewidth', 2);

figure();
