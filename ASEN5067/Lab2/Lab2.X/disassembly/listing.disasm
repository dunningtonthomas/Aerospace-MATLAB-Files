                                                  242:   // <editor-fold defaultstate="collapsed" desc="MACRO Definitions">			    
                                                  243:   ; MACRO Definitions:
                                                  244:   
                                                  245:   ; MOVLF
                                                  246:   ; Description:
                                                  247:   ;   Move literal value to given register. 
                                                  248:   ; Input: 
                                                  249:   ;   lit: literal value
                                                  250:   ;   dest: destination 
                                                  251:   ;   access: Access bank or not. Possible values are 'a' for access bank or
                                                  252:   ;	'b' for banked memory.
                                                  253:     MOVLF	    MACRO   lit, dest, access
                                                  254:       MOVLW   lit	    ; Move literal into WREG
                                                  255:       BANKSEL	(dest)	; Select Bank for next file instruction
                                                  256:       MOVWF   BANKMASK(dest), access  ; Move WREG into destination file
                                                  257:     ENDM
                                                  258:   // </editor-fold>
